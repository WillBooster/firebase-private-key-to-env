{
  "name": "firebase-private-key-to-env",
  "version": "1.0.0",
  "repository": "github:WillBooster/firebase-private-key-to-env",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": "src/index.ts",
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "postinstall": "husky install",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "start": "babel-node -x .js,.jsx,.es6,.es,.ts src/index.ts",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "core-js": "3.27.1",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "7.20.7",
    "@babel/core": "7.20.12",
    "@babel/node": "7.20.7",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-proposal-numeric-separator": "7.18.6",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-typescript": "7.18.6",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.18",
    "@types/yargs": "16.0.5",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "@willbooster/eslint-config-ts": "10.0.8",
    "@willbooster/prettier-config": "9.1.1",
    "@willbooster/renovate-config": "9.5.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-sort-class-members": "1.16.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.8.3",
    "sort-package-json": "2.1.0",
    "typescript": "4.9.4"
  },
  "packageManager": "yarn@4.0.0-rc.35",
  "publishConfig": {
    "access": "public"
  }
}
